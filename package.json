{
  "name": "innova-ul",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test:env": "npx dotenv-cli -e .env -- printenv TEST_VAR",
    "dev-expp": "npx nodemon server.js --ignore public/* --ignore src/*",
    "prepare:sessions": "node -e \"require('fs').mkdirSync('sessions', { recursive: true })\"",
    "dev": "npm run prepare:sessions & npm run dev-expp & npm run svelte",
    "svelte": "rollup -c -w",
    "dev:access": "npx dotenv-cli -e .env -- bash -c 'cd \"$ACCESS_PATH\" && npm run dev'",
    "dev:files": "npx dotenv-cli -e .env -- bash -c 'cd \"$FILES_PATH\" && npm run dev'",
    "dev-all": "concurrently -p \"[{name}]\" -n \"MAIN,ACCESS,FILES\" -c \"bgRed.bold,bgGreen.bold,bgBlue.bold,bgOrange.bold\" \"npm run dev\" \"npm run dev:access\" \"npm run dev:files\"",
    "db:new": "npx dbmate -d \"db/migrations\" -e \"DATABASE_URL\" new ",
    "db:up": "npx dbmate -d \"db/migrations\" -e \"DATABASE_URL\" up",
    "db:rollback": "npx dbmate -d \"db/migrations\" -e \"DATABASE_URL\" rollback",
    "db:dump": "npx dbmate -d \"db/migrations\" -e \"DATABASE_URL\" dump "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.6.0",
    "bootstrap": "^5.3.3",
    "concurrently": "^9.2.1",
    "connect-flash": "^0.1.1",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.9",
    "ejs-mate": "^4.0.0",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "font-awesome": "^4.7.0",
    "morgan": "^1.10.1",
    "mysql2": "^3.17.1",
    "session-file-store": "^1.5.0",
    "sirv-cli": "^2.0.0",
    "svelte": "^4.2.18",
    "svelte-app": "file:",
    "svelte-routing": "^2.13.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "dbmate": "^2.15.0",
    "dotenv-cli": "^11.0.0",
    "nodemon": "^3.0.1",
    "rollup": "^3.15.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.2",
    "svelte-preprocess": "^6.0.3"
  },
  "type": "module"
}
